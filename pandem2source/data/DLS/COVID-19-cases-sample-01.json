{
  "scope":{
    "source":"COVID-19 Catalonia Muni-ES",
    "source_description":"Casos diarios de COVID-19 en Catalonia por municipalidad",
    "tags":["COVID-19-DIARIOS MUNI-ES", "COVID-19-TESSY"],
    "frequency":"daily",
    "reference_user":"Elisa Lonnoc",
    "reporting_email":"e.lonnoc@pandem2.eu",
    "globals":[
      {"variable":"source"},
      {"variable":"pathogen", "value":"COVID-19"},
      {"variable":"population", "ref":"census.es.1997"}
    ],
    "update_scope":[
      {"variable":"source"},
      {"variable":"reporting_date"},
      {"variable":"geo", "value":"ES51", "ref":"EU.NUTS2"}
    ]
  },
  "acquisition":{
    "channel":{
      "name":"manual-upload"
    },
    "format": {
      "name":"csv",
      "decimal_sign":".",
      "thousands_separator":"",
      "date_format":"YYYY-MM-DD",
      "encoding":"UTF-8"
    }
  },
  "columns":[
    {"name":"Fecha de reporte", "variable":"reporting-date"},
    {"name":"Casos diarios", "variable":"confirmed-cases"},
    {"name":"Casos F0-12", "variable":"confirmed-cases", 
       "context":[
         {"variable":"age_group", "value":"0-12", "ref":"age-group-covid-eu.2021"},
         {"variable":"gender", "value":"F", "ref":"gender"}
       ]
    },
    {"name":"Casos F13-18", "variable":"confirmed-cases",
       "context":[
         {"variable":"age_group", "value":"13-18", "ref":"age-group-covid-eu.2021"},
         {"variable":"gender", "value":"F", "ref":"gender"}
       ]
    },
    {"name":"Casos F19-30", "variable":"confirmed-cases",
       "context":[
         {"variable":"age_group", "value":"19-30", "ref":"age-group-covid-eu.2021"},
         {"variable":"gender", "value":"F", "ref":"gender"}
       ]
    },
    {"name":"Casos F31-50", "variable":"confirmed-cases",
       "context":[
         {"variable":"age_group", "value":"31-50", "ref":"age-group-covid-eu.2021"},
         {"variable":"gender", "value":"F", "ref":"gender"}
       ]
    },
    {"name":"Casos F51-70", "variable":"confirmed-cases",
       "context":[
         {"variable":"age_group", "value":"51-70", "ref":"age-group-covid-eu.2021"},
         {"variable":"gender", "value":"F", "ref":"gender"}
       ]
    },
    {"name":"Casos F71+", "variable":"confirmed-cases",
       "context":[
