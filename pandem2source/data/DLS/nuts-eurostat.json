{
    "scope":{
      "source":"nuts-eurostat",
      "source_description":"",
      "tags":[],
      "frequency":"daily",
      "reference_user":"",
      "reporting_email":"",
      "globals":[
        {"variable":"source"},
        {"variable":"pathogen", "value":"COVID-19"}
      ],
      "update_scope":[
        {"variable":"source"},
        {"variable":"reporting_date"}
        
      ]
    },
    "acquisition":{
      "channel":{
        "name":"url",
        "url": "https://data.europa.eu/euodp/repository/ec/estat/nuts/nuts.rdf"
      },
      "format": {
        "row":"/rdf:RDF/rdf:Description"
      }
    },
    "columns":[
      {"name":"name", "variable":"location name", "xpath":"skos:prefLabel/text()"},
      {"name":"code", "variable":"location code", "xpath":"skos:notation/text()"},
      {"name":"parent", "variable":"location parent code", "xpath":"skos:broader/@rdf:resource", "find":"([^/]+)/?$"},
      {"name":"versions", "variable":"location versions", "xpath":"skos:inScheme/@rdf:resource", "find":"([^/]+)/?$"},
      {"name":"status", "variable":"location status", "xpath":"adms:status/@rdf:resource", "find":"([^/]+)/?$"}
    ]
  }